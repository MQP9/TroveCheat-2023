function skip() --performs the skip
  local xcoord=readFloat("[[[[[trove.exe+00FDF7B4]+0]+28]+C4]+4]+60") --Loading in the current xyz coords
  local ycoord=readFloat("[[[[[trove.exe+00FDF7B4]+0]+28]+C4]+4]+64")
  local zcoord=readFloat("[[[[[trove.exe+00FDF7B4]+0]+28]+C4]+4]+68")
  local xper=readFloat("[[[[[trove.exe+00FDF7B4]+4]+24]+84]+0]+100")
  local yper=readFloat("[[[[[trove.exe+00FDF7B4]+4]+24]+84]+0]+104")
  local zper=readFloat("[[[[[trove.exe+00FDF7B4]+4]+24]+84]+0]+108")
  local xadd=xper*3 --calculating distance to be traveled in each direction/currently this travels 3 blocks whichever direction you point, the 3 could be replaced with a differnt number or a variable
  local yadd=yper*3
  local zadd=zper*3
  local xdest=xcoord+xadd --calculating the new xyz coords
  local ydest=ycoord+yadd
  local zdest=zcoord+zadd
  writeFloat("[[[[[trove.exe+00FDF7B4]+0]+28]+C4]+4]+60",xdest) --Writes speed values
  writeFloat("[[[[[trove.exe+00FDF7B4]+0]+28]+C4]+4]+64",ydest)
  writeFloat("[[[[[trove.exe+00FDF7B4]+0]+28]+C4]+4]+68",zdest)
end

